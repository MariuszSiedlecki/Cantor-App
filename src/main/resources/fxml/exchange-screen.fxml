<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cantor.controllers.ExchangeController">
   <children>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <MenuButton fx:id="currencyMenuButtonFrom" layoutX="230.0" layoutY="60.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="waluta z"></MenuButton>
            <MenuButton fx:id="currencyMenuButtonTo" layoutX="330.0" layoutY="60.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="waluta na"></MenuButton>
            <TextField fx:id="currencyInputTextField" layoutX="109.0" layoutY="60.0" prefHeight="25.0" prefWidth="121.0" promptText="enter amount" />
            <Button fx:id="calculateButton" onAction="#calculate" layoutX="430.0" layoutY="60.0" mnemonicParsing="false" text="Button" />
            <TextArea fx:id="resultViewTextArea" layoutX="109.0" layoutY="103.0" prefHeight="271.0" prefWidth="374.0" wrapText="true" />
            <DatePicker fx:id="datePicker" layoutX="109.0" layoutY="22.0" prefHeight="25.0" prefWidth="121.0" promptText="data " />
         </children>
      </Pane>
   </children>
</StackPane>
